{
  "actions": [
    {
      "name": "Witsy: prompt",
      "type": "http",
      "method": "POST",
      "url": "{config.endpoint.witsy.base_url}{config.endpoint.witsy.trigger_path}",
      "headers": { "Content-Type": "application/json" },
      "json": { "cmd": "prompt", "text": "{text}" },
      "default_source": "primary"
    },
    {
      "name": "Witsy: command",
      "type": "http",
      "method": "POST",
      "url": "{config.endpoint.witsy.base_url}{config.endpoint.witsy.trigger_path}",
      "headers": { "Content-Type": "application/json" },
      "json": { "cmd": "command", "text": "{text}" },
      "default_source": "clipboard"
    },
    {
      "name": "Witsy: chat",
      "type": "http",
      "method": "GET",
      "url": "{config.endpoint.witsy.base_url}{config.endpoint.witsy.trigger_path}?cmd=chat"
    },
    {
      "name": "Witsy: scratchpad",
      "type": "http",
      "method": "GET",
      "url": "{config.endpoint.witsy.base_url}{config.endpoint.witsy.trigger_path}?cmd=scratchpad"
    },
    {
      "name": "Witsy: readaloud",
      "type": "http",
      "method": "GET",
      "url": "{config.endpoint.witsy.base_url}{config.endpoint.witsy.trigger_path}?cmd=readaloud"
    },
    {
      "name": "Witsy: transcribe",
      "type": "http",
      "method": "GET",
      "url": "{config.endpoint.witsy.base_url}{config.endpoint.witsy.trigger_path}?cmd=transcribe"
    },
    {
      "name": "Witsy: realtime",
      "type": "http",
      "method": "GET",
      "url": "{config.endpoint.witsy.base_url}{config.endpoint.witsy.trigger_path}?cmd=realtime"
    },
    {
      "name": "Witsy: studio",
      "type": "http",
      "method": "GET",
      "url": "{config.endpoint.witsy.base_url}{config.endpoint.witsy.trigger_path}?cmd=studio"
    },
    {
      "name": "Witsy: forge",
      "type": "http",
      "method": "GET",
      "url": "{config.endpoint.witsy.base_url}{config.endpoint.witsy.trigger_path}?cmd=forge"
    }
  ],
  "triggers": {
    "prompt": "Witsy: prompt",
    "command": "Witsy: command",
    "chat": "Witsy: chat",
    "scratchpad": "Witsy: scratchpad",
    "readaloud": "Witsy: readaloud",
    "transcribe": "Witsy: transcribe",
    "realtime": "Witsy: realtime",
    "studio": "Witsy: studio",
    "forge": "Witsy: forge"
  }
}
